{
  "expo": {
    "name": "BitSlice",
    "slug": "BitSlice",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/poro.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f2f2f2"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.BitSlice.VivemanArmy"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
